{
    "name": "@nodecfdi/sat-estado-cfdi-soap",
    "version": "2.0.0",
    "description": "Consulta el estado de un CFDI en el webservice del SAT usando SOAP (sin WSDL)",
    "keywords": [
        "SAT",
        "estado",
        "consulta",
        "cfdi",
        "servicio",
        "soap"
    ],
    "private": false,
    "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
    "license": "MIT",
    "type": "module",
    "main": "./dist/sat-estado-cfdi-soap.cjs",
    "module": "./dist/sat-estado-cfdi-soap.modern.js",
    "unpkg": "./dist/sat-estado-cfdi-soap.global.js",
    "browser": "./dist/sat-estado-cfdi-soap.global.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/sat-estado-cfdi-soap.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/sat-estado-cfdi-soap.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "is-ci || husky install",
        "lint": "eslint . --fix --ext .ts,.js",
        "lint:check": "eslint . --ext .ts,.js",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "build": "rimraf dist && tsup",
        "gen:docs": "rimraf docs && typedoc --options typedoc.json",
        "release": "pnpm run build && pnpm changeset publish"
    },
    "repository": "https://github.com/nodecfdi/sat-estado-cfdi-soap",
    "bugs": {
        "url": "https://github.com/nodecfdi/sat-estado-cfdi-soap/issues"
    },
    "homepage": "https://github.com/nodecfdi/sat-estado-cfdi-soap",
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "@commitlint/cli": "^17.6.7",
        "@nodecfdi/cfdi-expresiones": "^2.1.6",
        "@nodecfdi/eslint-config-nodecfdi": "^0.1.5",
        "@nrsk/config-conventional": "^1.0.0",
        "@types/is-ci": "^3.0.0",
        "@types/node": "^20.4.5",
        "@vitest/coverage-istanbul": "^0.33.0",
        "@xmldom/xmldom": "^0.8.10",
        "eslint": "^8.45.0",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "lint-staged": "^13.2.3",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "tsup": "^7.1.0",
        "typedoc": "^0.24.8",
        "typedoc-theme-hierarchy": "^4.0.0",
        "typescript": "^5.1.6",
        "vite": "^4.4.7",
        "vitest": "^0.33.0"
    },
    "peerDependencies": {
        "axios": ">=1.4.0"
    },
    "dependencies": {
        "@nodecfdi/cfdiutils-common": "^1.2.6",
        "@nodecfdi/sat-estado-cfdi": "^2.0.2"
    },
    "commitlint": {
        "extends": [
            "@nrsk/config-conventional"
        ]
    },
    "lint-staged": {
        "*.json": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "eslint --fix"
        ]
    },
    "engines": {
        "node": ">= 16"
    },
    "browserslist": [
        "defaults",
        "not IE 11"
    ]
}
